{
  "name": "@rokmohar/alexa-assistant",
  "version": "2.1.1",
  "description": "An unofficial Google skill for Alexa",
  "license": "GPL-3.0",
  "author": "Rok Mohar",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky",
    "build": "rimraf dist && npx webpack",
    "format": "prettier --check src/",
    "format:fix": "prettier --write src/",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "start": "serverless offline",
    "test": "serverless invoke local --function alexa",
    "logs": "serverless logs -f alexa"
  },
  "dependencies": {
    "@breezystack/lamejs": "^1.2.7",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "ask-sdk-core": "^2.14.0",
    "ask-sdk-dynamodb-persistence-adapter": "^2.14.0",
    "ask-sdk-model": "^1.86.0",
    "axios": "^1.13.4",
    "google-auth-library": "^9.15.1",
    "google-proto-files": "^5.0.1",
    "serverless": "^4.31.2",
    "serverless-alexa-skills": "^0.1.0",
    "serverless-offline": "^14.4.0",
    "serverless-webpack": "^5.15.3",
    "xml-escape": "^1.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.10",
    "@types/xml-escape": "^1.1.3",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "homepage": "https://github.com/rokmohar/alexa-assistant#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rokmohar/alexa-assistant.git"
  },
  "bugs": {
    "url": "https://github.com/rokmohar/alexa-assistant/issues"
  },
  "keywords": [
    "alexa",
    "skill",
    "google",
    "assistant"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
